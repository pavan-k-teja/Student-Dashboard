      <link rel='stylesheet prefetch' href='https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css'>
      <link rel="stylesheet" href="/todo/style.css">

      <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
	
      <link rel="stylesheet" href="/calender-todo/style.css">
  </head>
  <body id="body">
    <div class="container-dashboard">
      <nav class="navbar-dashboard">
        <div class="nav_icon" onclick="toggleSidebar()">
          <i class="fa fa-bars" aria-hidden="true"></i>
        </div>
        <div class="navbar__left">
          <!-- <a href="#">Subscribers</a>
          <a href="#">Video Management</a> -->
          <a class="active_link" href="#">Student</a>
        </div>
        <div class="alert alert-success alert-container" role="alert">
          <strong>Successfully Logged in!</strong> Have a nice day 😉
        </div>
        <div class="navbar__right">
          <a href="#">
            <img width="50" src="/dashboard/assets/avatar.png" alt="" />
            <!-- <i class="fa fa-user-circle-o" aria-hidden="true"></i> -->
          </a>
        </div>
      </nav>

      <main>
        <div class="main__container">
          <div class="main__title">
            <img src="/dashboard/assets/hello.svg" alt="" />
            <div class="main__greeting">
              <h1>Hello <%=name%></h1>
              <p>Welcome to your dashboard</p>
            </div>
          </div>
          <div class="todo-calender">
            <div class="todo-calender-child-1">
              <div class="today"></div>
              <div class="row">
                 <div class="item-todo-size">
                    <div class="add-control">
                       <div class="form-group has-feedback">
                          <input type="text" class="form-control" placeholder="✍️ Add item..."/>
                          <i class="fa fa-plus form-control-feedback add-btn" title="Add item"></i>
                       </div>
                    </div>
                    <!-- <p class="err text-danger text-center hidden"><i class="fa fa-warning"></i> Oops! Please, enter name item</p> -->
                    <p class="no-items text-muted text-center hidden"><i class="fa fa-ban"></i></p>
                    <ul class="todo-list">
                    </ul>
                 </div>
              </div>
           </div>
           <div class="todo-calender-child-2">
            <div class="col-md-12">
              <div class="elegant-calencar d-md-flex">
                <div class="wrap-header d-flex align-items-center img" style="background-image: url(/calender-todo/bg.jpg);">
                  <p id="reset">Today</p>
                  <div id="header" class="p-0">
                    <!-- <div class="pre-button d-flex align-items-center justify-content-center"><i class="fa fa-chevron-left"></i></div> -->
                    <div class="head-info">
                      <div class="head-month"></div>
                        <div class="head-day"></div>
                    </div>
                    <!-- <div class="next-button d-flex align-items-center justify-content-center"><i class="fa fa-chevron-right"></i></div> -->
                  </div>
                </div>
                <div class="calendar-wrap">
                  <div class="w-100 button-wrap">
                    <div class="pre-button d-flex align-items-center justify-content-center"><i class="fa fa-chevron-left"></i></div>
                    <div class="next-button d-flex align-items-center justify-content-center"><i class="fa fa-chevron-right"></i></div>
                  </div>
                  <table id="calendar">
                    <thead>
                        <tr>
                          <th>Sun</th>
                          <th>Mon</th>
                          <th>Tue</th>
                          <th>Wed</th>
                          <th>Thu</th>
                          <th>Fri</th>
                          <th>Sat</th>
                        </tr>
                    </thead>
                    <tbody>
                      <tr>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                      </tr>
                      <tr>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                      </tr>
                      <tr>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                      </tr>
                      <tr>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                      </tr>
                      <tr>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                      </tr>
                      <tr>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                      </tr>
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
           </div>
          </div>
        </div>
      </main>

      <div id="sidebar">
        <div class="sidebar__title">
          <div class="sidebar__img">
            <img src="/dashboard/assets/student.png" alt="logo" />
            <h1 style="font-weight: bold; color: #f3f4f6"><%=name%></h1>
          </div>
          <i
            onclick="closeSidebar()"
            class="fa fa-times"
            id="sidebarIcon"
            aria-hidden="true"
          ></i>
        </div>

        <div class="sidebar__menu">
          <div class="sidebar__link active_menu_link">
            <i class="fa fa-home"></i>
            <a href="/user/home">Dashboard</a>
          </div>
          <h2>Class</h2>
          <div class="sidebar__link">
            <i class="fa fa-user-secret" aria-hidden="true"></i>
            <a href="/user/attendance">Attendance</a>
          </div>
          <div class="sidebar__link">
            <i class="fa fa-building-o"></i>
            <a href="/user/timetable">Timetable</a>
          </div>
          <div class="sidebar__link">
            <i class="fa fa-wrench"></i>
            <a href="/user/almanac">Almanac</a>
          </div>
          <div class="sidebar__link">
            <i class="fa fa-archive"></i>
            <a href="/user/studyMaterial">Study Material</a>
          </div>
          <h2>College</h2>
          <div class="sidebar__link">
            <i class="fa fa-question"></i>
            <a href="/user/studentRequests/get">Student Requests</a>
          </div>
          <div class="sidebar__link">
            <i class="fa fa-sign-out"></i>
            <a href="/user/complaint/get">Complaints</a>
          </div>
          <div class="sidebar__link">
            <i class="fa fa-calendar-check-o"></i>
            <a href="/user/library">Library</a>
          </div>
          <div class="sidebar__link">
            <i class="fa fa-files-o"></i>
            <a href="/user/opportunity">Oppurtunities</a>
          </div>
          <div class="sidebar__link">
            <i class="fa fa-files-o"></i>
            <a href="/user/alumninetwork">Alumni Network</a>
          </div>
          
          <!-- <div class="sidebar__logout">
            <i class="fa fa-power-off"></i>
            <a href="#">Log out</a>
          </div> -->
          <div class="sidebar__logout">
            <i class="fa fa-power-off"></i>
            <a onclick="document.getElementById('logout').submit();" style="cursor: pointer;">Log out</a>
          </div>
        </div>
      </div>
    </div>
    <form id="logout" method="post" action="/user/logout"></form>
    <script src="https://cdn.jsdelivr.net/npm/apexcharts"></script>
    <script src="/dashboard/script.js"></script>

